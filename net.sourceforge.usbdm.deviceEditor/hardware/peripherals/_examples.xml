<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root SYSTEM "_menu.dtd" >
<!-- examples.xml -->

<fragment xmlns:xi="http://www.w3.org/2001/XInclude">
   <menu name="OSC0" description="Oscillator">
   
      <aliasOption key="system_mcgir_clock" />
   
      <choiceOption name="XXXX" description="YYYYY"
         toolTip="ZZZZZZZ">
         <choice value="0" name="AAAA" />
         <choice value="1" name="BBBB" />
         <choice value="2" name="CCCC" />
         <choice value="3" name="DDDD" />
      </choiceOption>

      <binaryOption name="XXXXX" description="YYYYY"
         toolTip="TTTTTTTTTTTT">
         <choice value="0" name="111111111111" />
         <choice value="1" name="222222222222" />
      </binaryOption>

      <intOption name="xxxx" description="yyyyyy" toolTip="yyyyyyy"
         value="4000000" constant="true" />
   </menu>
</fragment>
