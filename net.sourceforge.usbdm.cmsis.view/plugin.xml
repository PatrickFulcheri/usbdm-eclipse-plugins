<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Sample Category"
            id="net.sourceforge.usbdm.cmsis.view">
      </category>
      <view
            name="ProcessView"
            icon="icons/sample.gif"
            category="net.sourceforge.usbdm.cmsis.view"
            class="net.sourceforge.usbdm.cmsis.view.ProcessView"
            id="net.sourceforge.usbdm.cmsis.view.ProcessView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="net.sourceforge.usbdm.cmsis.view.ProcessView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
